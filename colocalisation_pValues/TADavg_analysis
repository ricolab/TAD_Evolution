library("biomaRt")
ensembl75 = useMart(host='feb2014.archive.ensembl.org',
						biomart='ENSEMBL_MART_ENSEMBL',
						dataset='hsapiens_gene_ensembl')
allBioMartHuman = getBM(mart = ensembl75, attributes = c("ensembl_gene_id", "wikigene_name", "chromosome_name", "start_position", "end_position", "gene_biotype"))

thisFolder = "/mnt/sexreg/temporal/geneage/"
sampleTADFile = paste0(thisFolder, "TADs_Neu_mean_merged.bed")

allBioMartHumanNoDups = allBioMartHuman[!duplicated(allBioMartHuman[, "ensembl_gene_id"]),]

essentialGenesListHuman = read.table("/mnt/sexreg/temporal/geneage/Human_Essential.txt")
geneEssListHuman = getGeneEssentialityTable(allBioMartHumanNoDups, essentialGenesListHuman) # equivalente a geneAgeList
TADList = as.data.frame(read.table(sampleTADFile, header = TRUE))

newTADList = getTADGenesTable(allBioMartHumanNoDups, TADList)
geneTADEssTableNoDups = mergeGeneTADAge(newTADList, geneEssListHuman) # equivalente a geneTADAgeTableAgeOnlyNoDups

# get a list {chromosome, TAD id, average age of contained genes in My}
TADEssList_binary = getTADAgeList(geneTADEssTableNoDups, essEquivalencies_binary) # equivalente a TADAgeList_binary

essJumpList_binary = getJumpList(TADEssList_binary, column = "meanAge") # equivalente a ageJumpList_binary
ggplot(essJumpList_binary, aes(x = jump)) + geom_histogram(aes(y = ..density.., colour = "black", fill = "white"), colour = "black", fill = "white", bins = 20) + geom_density(alpha = 0.2, fill = "#ff4040")

saveTADAgeGraphs(TADEssList_binary, folder = "/mnt/sexreg/temporal/geneage/plotEss/", filePrefix = "ess_binary_neu", fileSuffix = "real", ymax = 3.2)


# compare with randomisation of essentiality
