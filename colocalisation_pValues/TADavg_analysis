	print(paste0("Generating randomisation #", i, "."))
	TADEssList_binary_rnd = getTADAgeList(randomiseAllAges(geneTADEssTableNoDups), essEquivalencies_binary)
	essJumpList_binary_rnd = getJumpList(TADEssList_binary_rnd, column = "meanAge")
	print(paste0("Saving randomisation #", i, "."))
	saveTADAgeGraphs(TADEssList_binary_rnd, folder = "/mnt/sexreg/temporal/geneage/plotEss/", filePrefix = "ess_binary_neu", fileSuffix = paste0("rnd0", i), ymax = 3.2)
	}
	
# compare with jumps for different regular windows of 500000 bases
TADList_regular500 = generateRegularTADList(chromosomes_hg19, size = 500000)	
newTADList_regular500 = getTADGenesTable(allBioMartHumanNoDups, TADList_regular500)
geneTADEssTable_regular500 = mergeGeneTADAge(newTADList_regular500, geneEssListHuman)

TADEssList_binary_regular500 = getTADAgeList(geneTADEssTable_regular500, essEquivalencies_binary)
essJumpList_binary_regular500 = getJumpList(TADEssList_binary_regular500, column = "meanAge")
saveTADAgeGraphs(TADEssList_binary_regular500, folder = "/mnt/sexreg/temporal/geneage/plotEss/", filePrefix = "ess_binary_neu", fileSuffix = "reg500", ymax = 3.2)

