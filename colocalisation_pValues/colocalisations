write.table(ageNumbers_14abr_hESC_fused_essential, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_fused_essential.tsv", sep = "\t", row.names = FALSE, quote = FALSE)
ageNumbers_14abr_hESC_fused_nonEssential = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups_fused_nonEssential)
write.table(ageNumbers_14abr_hESC_fused_nonEssential, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_fused_nonEssential.tsv", sep = "\t", row.names = FALSE, quote = FALSE)




# repeating reducing the number of genes to check bias
