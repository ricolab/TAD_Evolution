write.table(ageNumbers_14abr_hESC_fused_nonEssential_reduced, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_fused_nonEssential_reduced.tsv", sep = "\t", row.names = FALSE, quote = FALSE)


