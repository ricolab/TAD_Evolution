neuTADs = read.table("/mnt/sexreg/temporal/geneage/TADs_Neu_mean_merged.bed", header = TRUE)

library(ggplot2)
library("biomaRt")
ensembl75 = useMart(host='feb2014.archive.ensembl.org',
						biomart='ENSEMBL_MART_ENSEMBL',
						dataset='hsapiens_gene_ensembl')
allBioMartHuman = getBM(mart = ensembl75, attributes = c("ensembl_gene_id", "wikigene_name", "chromosome_name", "start_position", "end_position", "gene_biotype"))

thisFolder = "/mnt/sexreg/temporal/geneage/"
sampleAgeFile = paste0(thisFolder, "AgesWithChrX.tsv") # converted from Caelinn's file in /data
geneAgeList = as.data.frame(read.table(sampleAgeFile, header = TRUE, sep = "\t"))
colnames(geneAgeList) = c("ensembl_gene_id", colnames(geneAgeList)[2:6])
geneAgeList = geneAgeList[,1:6]

newTADList = getTADGenesTable(allBioMartHuman, neuTADs)
geneTADAgeTable = mergeGeneTADAge(newTADList, geneAgeList)
geneTADAgeTableAgeOnly = subset(geneTADAgeTable, gene_age != "No Age Provided")
geneTADAgeTableAgeOnlyNoDups = geneTADAgeTableAgeOnly[!duplicated(geneTADAgeTableAgeOnly[, "ensembl_gene_id"]),]

ageList = as.data.frame(read.table(header = TRUE, text = "
age
FungiMetazoa
Bilateria
Chordata
Euteleostomi
Sarcopterygii
Tetrapoda
Amniota
Mammalia
Theria
Eutheria
Simiiformes
Catarrhini
Hominoidea
Hominidae
HomoPanGorilla
HomoSapiens
"))


####### final table, 14abr, neu, real
myFinalList_14abr_neu_real = getAllPairData(geneTADAgeTableAgeOnlyNoDups, ageList, totRandomisations = 15)
MLMatrix_14abr_neu_real = fromFinalListToSgnTable(myFinalList_14abr_neu_real)
write.table(MLMatrix_14abr_neu_real, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_neu_real.tsv", sep = "\t")

MLMatrix_14abr_neu_NH = fromFinalListToSgnTable(myFinalList_14abr_neu_real, getHugeTADdataInstead = TRUE)
write.table(MLMatrix_14abr_neu_NH, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_neu_NH.tsv", sep = "\t")

ageNumbers = ngeneInAge(ageList, makeAllOneHugeTAD(geneTADAgeTableAgeOnlyNoDups))
write.table(ageNumbers, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_neu_NH.tsv", sep = "\t", row.names = FALSE, quote = FALSE)

####### final table, 14abr, neu, iTR = inter TAD removed
neuTADs_iTR = interTADRemover(neuTADs, chromosomes_hg19)
newTADList_iTR = getTADGenesTable(allBioMartHuman, neuTADs_iTR)
geneTADAgeTable_iTR = mergeGeneTADAge(newTADList_iTR, geneAgeList)
geneTADAgeTableAgeOnly_iTR = subset(geneTADAgeTable_iTR, gene_age != "No Age Provided")
geneTADAgeTableAgeOnlyNoDups_iTR = geneTADAgeTableAgeOnly_iTR[!duplicated(geneTADAgeTableAgeOnly_iTR[, "ensembl_gene_id"]),]

myFinalList_14abr_neu_iTR = getAllPairData(geneTADAgeTableAgeOnlyNoDups_iTR, ageList, totRandomisations = 15)
MLMatrix_14abr_neu_iTR = fromFinalListToSgnTable(myFinalList_14abr_neu_iTR)
write.table(MLMatrix_14abr_neu_iTR, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_neu_iTR.tsv", sep = "\t")

####### final table, 14abr, neu, fused = fused TADs
neuTADs_fused = fusedTAD(neuTADs, chromosomes_hg19)
newTADList_fused = getTADGenesTable(allBioMartHuman, neuTADs_fused)
geneTADAgeTable_fused = mergeGeneTADAge(newTADList_fused, geneAgeList)
geneTADAgeTableAgeOnly_fused = subset(geneTADAgeTable_fused, gene_age != "No Age Provided")
geneTADAgeTableAgeOnlyNoDups_fused = geneTADAgeTableAgeOnly_fused[!duplicated(geneTADAgeTableAgeOnly_fused[, "ensembl_gene_id"]),]

myFinalList_14abr_neu_fused = getAllPairData(geneTADAgeTableAgeOnlyNoDups_fused, ageList, totRandomisations = 15)
MLMatrix_14abr_neu_fused = fromFinalListToSgnTable(myFinalList_14abr_neu_fused)
write.table(MLMatrix_14abr_neu_fused, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_neu_fused.tsv", sep = "\t")



#######################
#######################
