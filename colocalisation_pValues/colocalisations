####### final table, 14abr, mon, iTR = inter TAD removed
monTADs_iTR = interTADRemover(monTADs, chromosomes_hg19)
newTADList_iTR = getTADGenesTable(allBioMartHuman, monTADs_iTR)
geneTADAgeTable_iTR = mergeGeneTADAge(newTADList_iTR, geneAgeList)
geneTADAgeTableAgeOnly_iTR = subset(geneTADAgeTable_iTR, gene_age != "No Age Provided")
geneTADAgeTableAgeOnlyNoDups_iTR = geneTADAgeTableAgeOnly_iTR[!duplicated(geneTADAgeTableAgeOnly_iTR[, "ensembl_gene_id"]),]

myFinalList_14abr_mon_iTR = getAllPairData(geneTADAgeTableAgeOnlyNoDups_iTR, ageList, totRandomisations = 15)
MLMatrix_14abr_mon_iTR = fromFinalListToSgnTable(myFinalList_14abr_mon_iTR)
write.table(MLMatrix_14abr_mon_iTR, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_mon_iTR.tsv", sep = "\t")

ageNumbers = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups_iTR)
write.table(ageNumbers, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_mon_iTR.tsv", sep = "\t", row.names = FALSE, quote = FALSE)

####### final table, 14abr, mon, fused = fused TADs
monTADs_fused = fusedTAD(monTADs, chromosomes_hg19)
newTADList_fused = getTADGenesTable(allBioMartHuman, monTADs_fused)
geneTADAgeTable_fused = mergeGeneTADAge(newTADList_fused, geneAgeList)
geneTADAgeTableAgeOnly_fused = subset(geneTADAgeTable_fused, gene_age != "No Age Provided")
geneTADAgeTableAgeOnlyNoDups_fused = geneTADAgeTableAgeOnly_fused[!duplicated(geneTADAgeTableAgeOnly_fused[, "ensembl_gene_id"]),]

myFinalList_14abr_mon_fused = getAllPairData(geneTADAgeTableAgeOnlyNoDups_fused, ageList, totRandomisations = 15)
MLMatrix_14abr_mon_fused = fromFinalListToSgnTable(myFinalList_14abr_mon_fused)
write.table(MLMatrix_14abr_mon_fused, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_mon_fused.tsv", sep = "\t")

ageNumbers = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups_fused)
write.table(ageNumbers, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_mon_fused.tsv", sep = "\t", row.names = FALSE, quote = FALSE)



#######################
#######################
#######################
####################### Bcell ZONE
#######################
#######################

BcellTADs = read.table("/mnt/sexreg/temporal/geneage/GSE63525_GM12878_primary+replicate_Arrowhead_domainlist_sorted_noNested.txt", header = TRUE)

library(ggplot2)
library("biomaRt")
ensembl75 = useMart(host='feb2014.archive.ensembl.org',
						biomart='ENSEMBL_MART_ENSEMBL',
						dataset='hsapiens_gene_ensembl')
allBioMartHuman = getBM(mart = ensembl75, attributes = c("ensembl_gene_id", "wikigene_name", "chromosome_name", "start_position", "end_position", "gene_biotype"))

thisFolder = "/mnt/sexreg/temporal/geneage/"
sampleAgeFile = paste0(thisFolder, "AgesWithChrX.tsv") # converted from Caelinn's file in /data
geneAgeList = as.data.frame(read.table(sampleAgeFile, header = TRUE, sep = "\t"))
colnames(geneAgeList) = c("ensembl_gene_id", colnames(geneAgeList)[2:6])
geneAgeList = geneAgeList[,1:6]

newTADList = getTADGenesTable(allBioMartHuman, BcellTADs)
geneTADAgeTable = mergeGeneTADAge(newTADList, geneAgeList)
geneTADAgeTableAgeOnly = subset(geneTADAgeTable, gene_age != "No Age Provided")
geneTADAgeTableAgeOnlyNoDups = geneTADAgeTableAgeOnly[!duplicated(geneTADAgeTableAgeOnly[, "ensembl_gene_id"]),]

ageList = as.data.frame(read.table(header = TRUE, text = "
age
FungiMetazoa
Bilateria
Chordata
Euteleostomi
Sarcopterygii
Tetrapoda
Amniota
Mammalia
Theria
Eutheria
Simiiformes
Catarrhini
Hominoidea
Hominidae
HomoPanGorilla
HomoSapiens
"))


####### final table, 14abr, Bcell, real
myFinalList_14abr_Bcell_real = getAllPairData(geneTADAgeTableAgeOnlyNoDups, ageList, totRandomisations = 15)
MLMatrix_14abr_Bcell_real = fromFinalListToSgnTable(myFinalList_14abr_Bcell_real)
write.table(MLMatrix_14abr_Bcell_real, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_Bcell_real.tsv", sep = "\t")

MLMatrix_14abr_Bcell_NH = fromFinalListToSgnTable(myFinalList_14abr_Bcell_real, getHugeTADdataInstead = TRUE)
write.table(MLMatrix_14abr_Bcell_NH, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_Bcell_NH.tsv", sep = "\t")

ageNumbers = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups)
write.table(ageNumbers, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_Bcell_real.tsv", sep = "\t", row.names = FALSE, quote = FALSE)

ageNumbers = ngeneInAge(ageList, makeAllOneHugeTAD(geneTADAgeTableAgeOnlyNoDups))
write.table(ageNumbers, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_Bcell_NH.tsv", sep = "\t", row.names = FALSE, quote = FALSE)

####### final table, 14abr, Bcell, iTR = inter TAD removed
BcellTADs_iTR = interTADRemover(BcellTADs, chromosomes_hg19)
newTADList_iTR = getTADGenesTable(allBioMartHuman, BcellTADs_iTR)
geneTADAgeTable_iTR = mergeGeneTADAge(newTADList_iTR, geneAgeList)
geneTADAgeTableAgeOnly_iTR = subset(geneTADAgeTable_iTR, gene_age != "No Age Provided")
geneTADAgeTableAgeOnlyNoDups_iTR = geneTADAgeTableAgeOnly_iTR[!duplicated(geneTADAgeTableAgeOnly_iTR[, "ensembl_gene_id"]),]

myFinalList_14abr_Bcell_iTR = getAllPairData(geneTADAgeTableAgeOnlyNoDups_iTR, ageList, totRandomisations = 15)
MLMatrix_14abr_Bcell_iTR = fromFinalListToSgnTable(myFinalList_14abr_Bcell_iTR)
write.table(MLMatrix_14abr_Bcell_iTR, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_Bcell_iTR.tsv", sep = "\t")

ageNumbers = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups_iTR)
write.table(ageNumbers, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_Bcell_iTR.tsv", sep = "\t", row.names = FALSE, quote = FALSE)

####### final table, 14abr, Bcell, fused = fused TADs
BcellTADs_fused = fusedTAD(BcellTADs, chromosomes_hg19)
newTADList_fused = getTADGenesTable(allBioMartHuman, BcellTADs_fused)
geneTADAgeTable_fused = mergeGeneTADAge(newTADList_fused, geneAgeList)
geneTADAgeTableAgeOnly_fused = subset(geneTADAgeTable_fused, gene_age != "No Age Provided")
geneTADAgeTableAgeOnlyNoDups_fused = geneTADAgeTableAgeOnly_fused[!duplicated(geneTADAgeTableAgeOnly_fused[, "ensembl_gene_id"]),]

myFinalList_14abr_Bcell_fused = getAllPairData(geneTADAgeTableAgeOnlyNoDups_fused, ageList, totRandomisations = 15)
MLMatrix_14abr_Bcell_fused = fromFinalListToSgnTable(myFinalList_14abr_Bcell_fused)
write.table(MLMatrix_14abr_Bcell_fused, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_Bcell_fused.tsv", sep = "\t")

ageNumbers = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups_fused)
write.table(ageNumbers, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_Bcell_fused.tsv", sep = "\t", row.names = FALSE, quote = FALSE)
