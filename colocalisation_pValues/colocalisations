write.table(MLMatrix_14abr_hESC_NH_reduced, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_hESC_NH_reduced.tsv", sep = "\t")

ageNumbers_14abr_hESC_real_reduced = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups_reduced)
write.table(ageNumbers_14abr_hESC_real_reduced, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_real_reduced.tsv", sep = "\t", row.names = FALSE, quote = FALSE)

ageNumbers_14abr_hESC_NH_reduced = ngeneInAge(ageList, makeAllOneHugeTAD(geneTADAgeTableAgeOnlyNoDups_reduced))
write.table(ageNumbers_14abr_hESC_NH_reduced, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_NH_reduced.tsv", sep = "\t", row.names = FALSE, quote = FALSE)

####### final table, 14abr, hESC, iTR = inter TAD removed
hESCTADs_iTR = interTADRemover(hESCTADs, chromosomes_hg19)
newTADList_iTR = getTADGenesTable(allBioMartHuman, hESCTADs_iTR)
geneTADAgeTable_iTR = mergeGeneTADAge(newTADList_iTR, geneAgeList)
geneTADAgeTableAgeOnly_iTR = subset(geneTADAgeTable_iTR, gene_age != "No Age Provided")
geneTADAgeTableAgeOnlyNoDups_iTR = geneTADAgeTableAgeOnly_iTR[!duplicated(geneTADAgeTableAgeOnly_iTR[, "ensembl_gene_id"]),]

myFinalList_14abr_hESC_iTR = getAllPairData(geneTADAgeTableAgeOnlyNoDups_iTR, ageList, totRandomisations = 15)
MLMatrix_14abr_hESC_iTR = fromFinalListToSgnTable(myFinalList_14abr_hESC_iTR)
write.table(MLMatrix_14abr_hESC_iTR, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_hESC_iTR.tsv", sep = "\t")

probDiffMatrix_14abr_hESC_iTR = fromFinalListToProbDiffTable(myFinalList_14abr_hESC_iTR)
write.table(probDiffMatrix_14abr_hESC_iTR, "/mnt/sexreg/temporal/geneage/probDiffRatio_table_14abr_hESC_iTR.tsv", sep = "\t")


essentialList = read.table("/mnt/sexreg/temporal/geneage/Human_Essential.txt", header = FALSE, sep = "\t")
geneTADAgeTableAgeOnlyNoDups_iTR_essential = filterTableWithList(geneTADAgeTableAgeOnlyNoDups_iTR, essentialList, filtering = "inclusion")
geneTADAgeTableAgeOnlyNoDups_iTR_nonEssential = filterTableWithList(geneTADAgeTableAgeOnlyNoDups_iTR, essentialList, filtering = "exclusion")

myFinalList_14abr_hESC_iTR_essential = getAllPairData(geneTADAgeTableAgeOnlyNoDups_iTR_essential, ageList, totRandomisations = 15)
MLMatrix_14abr_hESC_iTR_essential = fromFinalListToSgnTable(myFinalList_14abr_hESC_iTR_essential)
write.table(MLMatrix_14abr_hESC_iTR_essential, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_hESC_iTR_essential.tsv", sep = "\t")


myFinalList_14abr_hESC_iTR_nonEssential = getAllPairData(geneTADAgeTableAgeOnlyNoDups_iTR_nonEssential, ageList, totRandomisations = 15)
MLMatrix_14abr_hESC_iTR_nonEssential = fromFinalListToSgnTable(myFinalList_14abr_hESC_iTR_nonEssential)
write.table(MLMatrix_14abr_hESC_iTR_nonEssential, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_hESC_iTR_nonEssential.tsv", sep = "\t")

ageNumbers_14abr_hESC_iTR_essential = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups_iTR_essential)
write.table(ageNumbers_14abr_hESC_iTR_essential, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_iTR_essential.tsv", sep = "\t", row.names = FALSE, quote = FALSE)
ageNumbers_14abr_hESC_iTR_nonEssential = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups_iTR_nonEssential)
write.table(ageNumbers_14abr_hESC_iTR_nonEssential, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_iTR_nonEssential.tsv", sep = "\t", row.names = FALSE, quote = FALSE)




# repeating reducing the number of genes to check bias
geneTADAgeTableAgeOnlyNoDups_iTR_nonEssential_reduced = tableReducer(geneTADAgeTableAgeOnlyNoDups_iTR_nonEssential, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_iTR_essential.tsv")

myFinalList_14abr_hESC_iTR_nonEssential_reduced = getAllPairData(geneTADAgeTableAgeOnlyNoDups_iTR_nonEssential_reduced, ageList, totRandomisations = 15)
MLMatrix_14abr_hESC_iTR_nonEssential_reduced = fromFinalListToSgnTable(myFinalList_14abr_hESC_iTR_nonEssential_reduced)
write.table(MLMatrix_14abr_hESC_iTR_nonEssential_reduced, "/mnt/sexreg/temporal/geneage/minus_LogPValueSgn_table_14abr_hESC_iTR_nonEssential_reduced.tsv", sep = "\t")

ageNumbers_14abr_hESC_iTR_nonEssential_reduced = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups_iTR_nonEssential_reduced)
write.table(ageNumbers_14abr_hESC_iTR_nonEssential_reduced, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_iTR_nonEssential_reduced.tsv", sep = "\t", row.names = FALSE, quote = FALSE)

# repeating reducing the number of genes to check bias - basic
geneTADAgeTableAgeOnlyNoDups_iTR_reduced = tableReducer(geneTADAgeTableAgeOnlyNoDups_iTR, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_iTR_essential.tsv")

myFinalList_14abr_hESC_iTR_reduced = getAllPairData(geneTADAgeTableAgeOnlyNoDups_iTR_reduced, ageList, totRandomisations = 15)
MLMatrix_14abr_hESC_iTR_reduced = fromFinalListToSgnTable(myFinalList_14abr_hESC_iTR_reduced)
write.table(MLMatrix_14abr_hESC_iTR_reduced, "/mnt/sexreg/temporal/geneage/minus_LogPValueSgn_table_14abr_hESC_iTR_reduced.tsv", sep = "\t")

ageNumbers_14abr_hESC_iTR_reduced = ngeneInAge(ageList, geneTADAgeTableAgeOnlyNoDups_iTR_reduced)
write.table(ageNumbers_14abr_hESC_iTR_reduced, "/mnt/sexreg/temporal/geneage/ageNumbers_14abr_hESC_iTR_reduced.tsv", sep = "\t", row.names = FALSE, quote = FALSE)


####### final table, 14abr, hESC, fused = fused TADs
hESCTADs_fused = fusedTAD(hESCTADs, chromosomes_hg19)
newTADList_fused = getTADGenesTable(allBioMartHuman, hESCTADs_fused)
geneTADAgeTable_fused = mergeGeneTADAge(newTADList_fused, geneAgeList)
geneTADAgeTableAgeOnly_fused = subset(geneTADAgeTable_fused, gene_age != "No Age Provided")
geneTADAgeTableAgeOnlyNoDups_fused = geneTADAgeTableAgeOnly_fused[!duplicated(geneTADAgeTableAgeOnly_fused[, "ensembl_gene_id"]),]

myFinalList_14abr_hESC_fused = getAllPairData(geneTADAgeTableAgeOnlyNoDups_fused, ageList, totRandomisations = 15)
MLMatrix_14abr_hESC_fused = fromFinalListToSgnTable(myFinalList_14abr_hESC_fused)
write.table(MLMatrix_14abr_hESC_fused, "/mnt/sexreg/temporal/geneage/minusLogPValueSgn_table_14abr_hESC_fused.tsv", sep = "\t")

probDiffMatrix_14abr_hESC_fused = fromFinalListToProbDiffTable(myFinalList_14abr_hESC_fused)
write.table(probDiffMatrix_14abr_hESC_fused, "/mnt/sexreg/temporal/geneage/probDiffRatio_table_14abr_hESC_fused.tsv", sep = "\t")

essentialList = read.table("/mnt/sexreg/temporal/geneage/Human_Essential.txt", header = FALSE, sep = "\t")
geneTADAgeTableAgeOnlyNoDups_fused_essential = filterTableWithList(geneTADAgeTableAgeOnlyNoDups_fused, essentialList, filtering = "inclusion")
geneTADAgeTableAgeOnlyNoDups_fused_nonEssential = filterTableWithList(geneTADAgeTableAgeOnlyNoDups_fused, essentialList, filtering = "exclusion")

